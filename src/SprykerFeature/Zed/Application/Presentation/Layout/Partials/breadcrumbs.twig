<ul class="breadcrumb">
{# @var $page Zend_Navigation_Page_Mvc #}
<?php foreach ($this->pages as $page): ?>
    {% set classActive = page.getIsActive ? 'active' : '' %}
    <li class="{{ classActive }}">
        <a href="{{ page.href }}">
            {{ page.label }}
        </a>
    </li>
<?php endforeach; ?>
</ul>
